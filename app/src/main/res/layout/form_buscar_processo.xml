<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="fill_parent" android:layout_width="fill_parent"
    android:background="#ffffff">
    <TableRow>
        <TextView android:id="@+id/text1" android:text="Nome:"
            android:textColor="#000000" />
        <EditText android:id="@+id/etNomeProcesso" android:width="240px" />
    </TableRow>
    <TableRow>
        <ImageButton android:src="@drawable/pesquisar"
            android:id="@+id/btBuscar" android:text="Buscar" />
    </TableRow>
    <TableRow>
        <TextView android:id="@+id/text2" android:text="Responsável:"
            android:textColor="#000000" />
        <EditText android:id="@+id/etResponsavel"  />
    </TableRow>
    <TableRow>
        <TextView android:id="@+id/text3" android:text="Papel:"
            android:textColor="#000000" />
        <EditText android:id="@+id/etPapel"  />
    </TableRow>
    <TableRow>
        <TextView android:id="@+id/text4" android:text="Objetivo:"
            android:textColor="#000000" />
        <EditText android:id="@+id/etObjetivo"  />
    </TableRow>
    <TableRow>
        <TextView android:id="@+id/text5" android:text="Entradas:"
            android:textColor="#000000" />
        <EditText android:id="@+id/etEntradas"  />
    </TableRow>
    <TableRow>
        <TextView android:id="@+id/text6" android:text="Saídas:"
            android:textColor="#000000" />
        <EditText android:id="@+id/etSaidas"  />
    </TableRow>
    <TableRow>
        <TextView android:id="@+id/text7" android:text="Roteiro:"
            android:textColor="#000000" />
        <EditText android:id="@+id/etRoteiro"  />
    </TableRow>

</TableLayout>